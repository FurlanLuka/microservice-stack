{
  "$schema": "http://json-schema.org/schema",
  "name": "microservices",
  "version": "0.0.3",
  "generators": {
    "configuration-generator": {
      "factory": "./src/generators/configuration-generator/generator",
      "schema": "./src/generators/configuration-generator/schema.json",
      "description": "Microservice stack configuration generator"
    },
    "application-generator": {
      "factory": "./src/generators/app-generator/generator",
      "schema": "./src/generators/app-generator/schema.json",
      "description": "Microservice application generator"
    },
    "util-library": {
      "factory": "./src/generators/util-library-generator/generator",
      "schema": "./src/generators/util-library-generator/schema.json",
      "description": "Util library generator"
    }
  }
}
