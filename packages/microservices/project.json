{
  "name": "microservices",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/microservices/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/microservices",
        "main": "packages/microservices/src/index.ts",
        "tsConfig": "packages/microservices/tsconfig.lib.json",
        "assets": [
          "packages/microservices/*.md",
          {
            "input": "./packages/microservices/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./packages/microservices/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./packages/microservices",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./packages/microservices",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/microservices/**/*.ts",
          "packages/microservices/generators.json",
          "packages/microservices/executors.json",
          "packages/microservices/package.json"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/microservices/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
